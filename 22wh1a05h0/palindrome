package cseb;

public class exception {
public static void main(String[]args) {
	 String str="abc";
	 String rev="";
	 char ch;
	 for(int i=str.length()-1;i>=0;i--) {
		 ch=str.charAt(i);
		 rev=rev+ch;
	}
	 if(str==rev) {
		 System.out.println("palindrome");
		 
	 }
	 else {
		 System.out.println("not palindrome");

	 }
	
}
}
